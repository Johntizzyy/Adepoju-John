    "db:push": "drizzle-kit push"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.0.0",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/ws": "^8.5.13",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.30.4",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
